            "type": "object",
            "required": [
                "nasname",
                "secret"
            ],
            "properties": {
                "community": {
                    "type": "string",
                    "maxLength": 50
                },
                "description": {
                    "type": "string",
                    "maxLength": 200
                },
                "limit_proxy_state": {
                    "type": "string",
                    "maxLength": 4
                },
                "nasname": {
                    "type": "string",
                    "maxLength": 128
                },
                "ports": {
                    "type": "integer"
                },
                "require_ma": {
                    "type": "string",
                    "maxLength": 4
                },
                "secret": {
                    "type": "string",
                    "maxLength": 60
                },
                "server": {
                    "type": "string",
                    "maxLength": 64
                },
                "shortname": {
                    "type": "string",
                    "maxLength": 32
                },
                "type": {
                    "type": "string",
                    "maxLength": 30
                }
            }
        },
        "dto.CreatePaymentRequest": {
